{
  "name": "ags-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cp ./env/.env.development .env.local && NODE_TLS_REJECT_UNAUTHORIZED='0' && npm run start:https",
    "build": "cp ./env/.env.production .env.local && next build",
    "docker": "npm run build:docker && npm run start:docker",
    "build:docker": "cp ./env/.env.docker .env.local && next build",
    "start": "cp ./env/.env.production .env.local &&  && NODE_ENV=production && next start",
    "start:docker": "cp ./env/.env.docker .env.local && NODE_TLS_REJECT_UNAUTHORIZED='0' && NODE_ENV=docker && npm run start:https",
    "start:https": "node server.development.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@types/node": "^16.11.7",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "cookies": "^0.8.0",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "next": "10.0.1",
    "openid-client": "^4.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reactstrap": "^8.7.1",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@types/cookies": "^0.7.7",
    "@types/openid-client": "^3.7.0",
    "@types/react": "^17.0.45",
    "typescript": "^4.6.4"
  }
}
